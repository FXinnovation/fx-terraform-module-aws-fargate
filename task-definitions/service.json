[
  {
    "name": "${container_name}",
    "image": "${container_image}",
    "cpu": "${container_cpu_limit}",
    "memory": "${container_mem_limit}",
    "essential": true,
    "portMappings": [
      {
        "containerPort": "${container_inside_port}",
        "hostPort": "${container_outside_port}"
      }
    ]
  }
]
